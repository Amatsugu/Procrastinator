<html>
	<head>
		<link href="https://fonts.googleapis.com/css?family=Roboto:100,300" rel="stylesheet">
		<meta name="viewport" content="width=device-width, initial-scale=1" />
		
		<link rel="stylesheet" href="/res/css/main.css">
		<link rel="stylesheet" href="/res/css/login.css">
		<link rel="stylesheet" href="/res/css/scrollBar.css">
		<link rel="stylesheet" type="text/css" href="/res/css/smallScreen.css" media="only screen and (max-width: 1030px)" />
		<link rel="stylesheet" type="text/css" href="/res/css/smallerScreen.css" media="only screen and (max-width: 500px)" />
		<script src="/res/js/jquery-3.2.1.min.js" type="text/javascript"></script>
		<script async src="/res/js/login.js"></script>
		<title>Procrastinator</title>
	</head>
	<body>
		<div id="pageWrap">
			<div id="loginWindow">
				<div id="loginBlurBg">
					<div id="loginPanel">
						<form name="login">
							<img id="logoImg" src="/res/img/logo.png">
							<label id="logo">Procrastinator</label>
							<label id="formError"></label>
							<input type="text" name="username" placeholder="Username">
							<input type="password" name="password" placeholder="Password">
							<a href="/register">Register</a>
							<input type="submit" value="Login">
						</form>
					</div>
				</div>
			</div>
		</div>
	</body>
</html>